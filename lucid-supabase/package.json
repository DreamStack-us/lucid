{
  "name": "@dreamstack-us/lucid-supabase",
  "version": "0.1.0",
  "description": "Supabase connector for Lucid.js - sync with Supabase backend",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@dreamstack-us/lucid-core": "^0.1.0"
  },
  "peerDependencies": {
    "@supabase/supabase-js": ">=2.0.0"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.46.0",
    "tsup": "^8.3.5",
    "typescript": "~5.3.3"
  },
  "keywords": [
    "lucid",
    "supabase",
    "sync",
    "offline-first"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
